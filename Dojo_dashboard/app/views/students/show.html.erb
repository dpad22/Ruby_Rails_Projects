<h1><%= @dojo.name %>'s <%= @students.fname %> <%= @students.lname %></h1>
<h3>Email: <%= @students.email %> </h3>
<h3><%= @dojo.city %>, <%= @dojo.state %></h3>
<hr>

<h2>Cohort</h2>
<table>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Student ID</th>
    <% @dojo.students.each do |student| %>
        <tr> 
            <td><%= student.fname %> </td>
            <td><%= student.lname %> </td>
            <td><%= student.email %> </td>
            <td><%= student.id %> </td>
        </tr>
    <% end %>
</table>

